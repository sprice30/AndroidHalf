<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.samprice.applytojob.MainActivity">


<GridLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_alignParentTop="true"
    android:layout_centerHorizontal="true"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/gridLayout2">

    <TextView
        android:layout_width="164dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="@string/r1"
        android:id="@+id/r1"
        android:layout_row="1"
        android:layout_column="0" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="101dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:id="@+id/textView46"
        android:layout_row="0"
        android:layout_column="0" />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="69dp"
        android:id="@+id/view1"
        android:text="View"
        android:onClick="onClickView1"
        android:layout_row="1"
        android:layout_column="1" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClickTrash1"
        android:id="@+id/trash1"
        android:src="@mipmap/trash"
        android:layout_row="1"
        android:layout_column="2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="@string/r2"
        android:id="@+id/r2"
        android:layout_row="2"
        android:layout_column="0" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="69dp"
        android:onClick="onClickView2"
        android:text="View"
        android:id="@+id/view2"
        android:layout_row="2"
        android:layout_column="1" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClickTrash2"
        android:id="@+id/trash2"
        android:src="@mipmap/trash"
        android:layout_row="2"
        android:layout_column="2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="@string/r3"
        android:id="@+id/r3"
        android:layout_row="3"
        android:layout_column="0" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="67dp"
        android:text="View"
        android:onClick="onClickView3"
        android:id="@+id/view3"
        android:layout_row="3"
        android:layout_column="1" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClickTrash3"
        android:id="@+id/trash3"
        android:src="@mipmap/trash"
        android:layout_row="3"
        android:layout_column="2" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="@string/r4"
        android:id="@+id/r4"
        android:layout_row="4"
        android:layout_column="0" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="67dp"
        android:text="View"
        android:id="@+id/view4"
        android:onClick="onClickView4"
        android:layout_row="4"
        android:layout_column="1" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClickTrash4"
        android:id="@+id/trash4"
        android:src="@mipmap/trash"
        android:layout_row="4"
        android:layout_column="2" />


</GridLayout>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="    Uploaded Resumes"
        android:textSize="30sp"
        android:id="@+id/textView52"
        android:layout_row="0"
        android:layout_column="0"
        android:layout_columnSpan="3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />
    <Button
        android:layout_width="110dp"
        android:layout_height="90dp"
        android:text="Post New Resume"
        android:id="@+id/button8"
        android:onClick="onClickResumePost"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />
    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Back"
        android:id="@+id/button15"
        android:onClick="onClickMain"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

</RelativeLayout>